{
    "compilerOptions": {
      "outDir": "./dist/",
      "module": "esnext",
      "target": "es5",
      "lib": ["dom", "es5", "es6"],
      "moduleResolution": "node",
      "rootDir": "./src/",
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "suppressImplicitAnyIndexErrors": true,
      "noUnusedLocals": true,
      "baseUrl": "./",
    },
    "exclude": [
      "node_modules",
    ]
  }
  